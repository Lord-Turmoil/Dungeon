            <!-- Level prompt (s for small, b for big/banner)-->
            <Static name="s-level" pos="320 20">
                <Drawer>
                    <Text width="240" height="40" focus="120 0" font-size="30" font="8bitoperator JVE Regular" color="#FFFFFF">Level 0 - 0</Text>
                </Drawer>
            </Static>
            <Static name="b-level" pos="320 240">
                <Drawer>
                    <Text width="640" height="100" focus="320 50" font-size="100" font="8bitoperator JVE Regular" color="#FFFFFF">&gt; Level 0 - 0 &lt;</Text>
                </Drawer>
                <Transitions>
                    <AlphaTransition duration="500" delay="1000" loop="false" reverse="false" style="ease-in" begin="255" end="0"></AlphaTransition>
                    <AlphaTransition duration="500" delay="0" loop="false" reverse="false" style="linear" begin="0" end="255"></AlphaTransition>
                    <ScaleTransition duration="500" delay="1000" loop="false" reverse="false" style="ease-in" begin="1.0" end="5.0"></ScaleTransition>
                </Transitions>
            </Static>
            <Static name="b-complete" pos="320 240">
                <Drawer>
                    <Text width="640" height="100" focus="320 50" font-size="100" font="8bitoperator JVE Regular" color="#FFFFFF">&gt; Complete &lt;</Text>
                </Drawer>
                <Transitions>
                    <AlphaTransition duration="500" delay="1000" loop="false" reverse="false" style="ease-in" begin="255" end="0"></AlphaTransition>
                    <AlphaTransition duration="500" delay="0" loop="false" reverse="false" style="linear" begin="0" end="255"></AlphaTransition>
                </Transitions>
            </Static>
            <Static name="b-final" pos="320 240">
