                    <Buzzer src="hover"></Buzzer>
                </HoverSound>
                <ClickSound>
                    <Buzzer src="click"></Buzzer>
                </ClickSound>
            </Button>
            <!-- Select List -->
            <RadioList name="hero">
                <Radio name="yony" pos="105 140">
                    <Box>
                        <Button name="" float="true" pos="0% 0%">
                            <Up>
                                <Image width="150" height="240" origin="0 0" size="150 240" src="select"></Image>
                            </Up>
                            <Hover>
                                <Image width="150" height="240" origin="150 0" size="150 240" src="select"></Image>
                            </Hover>
                            <Down>
                                <Image width="150" height="240" origin="300 0" size="150 240" src="select"></Image>
                            </Down>
                            <Trigger>
                                <Trigger type="mask" width="150" height="240" origin="450 0" size="150 240" src="select"></Trigger>
                            </Trigger>
                            <HoverSound>
                                <Buzzer src="hover"></Buzzer>
                            </HoverSound>
                            <ClickSound>
                                <Buzzer src="yy-slt"></Buzzer>
                            </ClickSound>
                        </Button>
                    </Box>
                    <Mark>
                        <Image width="150" height="240" origin="600 0" size="150 240" src="select"></Image>
                    </Mark>
                </Radio>
                <Radio name="blck" pos="385 140">
                    <Box>
                        <Button name="" float="true" pos="0% 0%">
                            <Up>
                                <Image width="150" height="240" origin="0 240" size="150 240" src="select"></Image>
                            </Up>
                            <Hover>
                                <Image width="150" height="240" origin="150 240" size="150 240" src="select"></Image>
                            </Hover>
                            <Down>
                                <Image width="150" height="240" origin="300 240" size="150 240" src="select"></Image>
                            </Down>
                            <Trigger>
                                <Trigger type="mask" width="150" height="240" origin="450 240" size="150 240" src="select"></Trigger>
                            </Trigger>
                            <HoverSound>
                                <Buzzer src="hover"></Buzzer>
                            </HoverSound>
                            <ClickSound>
                                <Buzzer src="bk-slt"></Buzzer>
                            </ClickSound>
                        </Button>
                    </Box>
                    <Mark>
                        <Image width="150" height="240" origin="600 240" size="150 240" src="select"></Image>
                    </Mark>
                </Radio>
            </RadioList>
            <!-- Black Purchase -->
            <Button name="buy-blck" pos="385 140">
                <Up>
                    <Image width="150" height="240" origin="0 480" size="150 240" src="select"></Image>
                </Up>
                <Hover>
                    <Image width="150" height="240" origin="150 480" size="150 240" src="select"></Image>
                </Hover>
                <Down>
                    <Image width="150" height="240" origin="300 480" size="150 240" src="select"></Image>
                </Down>
                <Disabled>
                    <Image width="150" height="240" origin="450 480" size="150 240" src="select"></Image>
                </Disabled>
                <Trigger>
                    <Trigger type="mask" width="150" height="240" origin="600 480" size="150 240" src="select"></Trigger>
                </Trigger>
                <HoverSound>
                    <Buzzer src="hover"></Buzzer>
                </HoverSound>
                <ClickSound>
                    <Buzzer src="click"></Buzzer>
                </ClickSound>
