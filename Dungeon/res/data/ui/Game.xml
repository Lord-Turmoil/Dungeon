                    <Roundrect width="90" height="12" radius="10" filled="false" line-color="rgb(62, 39, 35)" border="2"></Roundrect>
                </Bar>
                <Stuff>
                    <Roundrect width="90" height="12" focus="-1 -1" radius="10" fill-color="rgb(21, 101, 192)" border="0"></Roundrect>
