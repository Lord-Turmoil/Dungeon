                <Transitions>
                    <ScaleTransition duration="500" delay="0" loop="true" reverse="true" style="linear" begin="1.0" end="1.1"></ScaleTransition>
                </Transitions>
            </Static>

            <!-- Weapon prompt -->
            <Static name="weapon-back" float="true" pos="100% 100%" alpha="128">
                <Drawer>
                    <Circle radius="100" focus="100 100" color="#000000">
                    </Circle>
