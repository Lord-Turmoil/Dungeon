                    <ScaleTransition duration="1300" delay="123" loop="true" reverse="true" style="ease-out" begin="1.0" end="0.8"></ScaleTransition>
                    <AlphaTransition duration="1300" delay="123" loop="true" reverse="true" style="linear" begin="255" end="128"></AlphaTransition>
                </Transitions>
            </Static>
            <Static name="sun" pos="580 200" layer="5">
                <Drawer>
                    <Image width="128" height="128" focus="64 64" src="sun" size="48 48"></Image>
                </Drawer>
            </Static>
            <!-- Title -->
            <Static name="title" pos="320 240" layer="10">
                <Drawer>
                    <Text width="640" height="80" focus="320 40" font-size="80" font="8bitoperator JVE Regular" color="#000000">You Have Escaped</Text>
                </Drawer>
            </Static>
        </Widgets>
    </Time>
